{
  "name": "trade-show-app",
  "private": true,
  "version": "1.31.3",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "prebuild:sandbox": "node scripts/validate-env.js --mode development",
    "build:sandbox": "vite build --mode development",
    "prebuild:production": "node scripts/validate-env.js --mode production",
    "build:production": "vite build --mode production",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "preview": "vite preview",
    "start:backend": "cd backend && npm run dev",
    "start:all": "concurrently \"npm run start:backend\" \"npm run dev\" --names \"backend,frontend\" --prefix-colors \"blue,green\""
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.57.4",
    "axios": "^1.6.5",
    "dexie": "^4.2.1",
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "concurrently": "^8.2.2",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2"
  }
}
